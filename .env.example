# n8n AI Starter Kit Environment Configuration

# Domain settings
DOMAIN=your-domain.com

# n8n Configuration
N8N_PROTOCOL=https
N8N_HOST=your-domain.com
WEBHOOK_URL=https://your-domain.com
WEBHOOK_TUNNEL_URL=https://your-domain.com

# n8n authentication
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=your-email@domain.com
N8N_BASIC_AUTH_PASSWORD=your-secure-password

# Database settings
POSTGRES_USER=root
POSTGRES_PASSWORD=your-secure-db-password
POSTGRES_DB=n8n

# n8n encryption and security
N8N_ENCRYPTION_KEY=your-32-character-encryption-key
N8N_USER_MANAGEMENT_JWT_SECRET=your-jwt-secret-key

# File storage
N8N_DEFAULT_BINARY_DATA_MODE=filesystem

# Ollama settings (for AI features)
OLLAMA_HOST=ollama:11434

# Other n8n settings
N8N_DIAGNOSTICS_ENABLED=false
N8N_PERSONALIZATION_ENABLED=false
N8N_VERSION_NOTIFICATIONS_ENABLED=false
N8N_TEMPLATES_ENABLED=false
N8N_ONBOARDING_FLOW_DISABLED=true

# Generic timezone
GENERIC_TIMEZONE=UTC
N8N_ENFORCE_SETTINGS_FILE_PERMISSIONS=true
N8N_SECURE_COOKIE=true

# For Mac users running OLLAMA locally
# See https://github.com/n8n-io/self-hosted-ai-starter-kit?tab=readme-ov-file#for-mac--apple-silicon-users
# OLLAMA_HOST=host.docker.internal:11434


